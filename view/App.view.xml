<mvc:View
    controllerName="ns.quickstartproject1.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
                    <Button text="Ready" type="Emphasized" class="sapUiSmallMarginBeginEnd" press="onreadybtn"/>
                    <Button text="Steady" type="Emphasized" class="sapUiSmallMarginBeginEnd"/>
                    <Button text="Go" type="Emphasized" class="sapUiSmallMarginBeginEnd"/> 
                    </content>

				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
